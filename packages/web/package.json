{
  "name": "@raft/web",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "yarn build:css && yarn build:remix",
    "build:remix": "remix build",
    "build:css": "tailwindcss -m -i ./app/tw.css -o app/styles.css",
    "dev": "yarn build:remix && run-p \"dev:*\"",
    "dev:remix": "remix watch",
    "dev:wrangler": "NODE_ENV=development npm run wrangler",
    "dev:css": "tailwindcss -w -i ./app/tw.css -o app/styles.css",
    "start": "NODE_ENV=production npm run wrangler",
    "typecheck": "tsc --noEmit",
    "wrangler": "wrangler pages dev ./public"
  },
  "dependencies": {
    "@remix-run/cloudflare": "1.11.1",
    "@remix-run/cloudflare-pages": "1.11.1",
    "@remix-run/react": "1.11.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.2.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "3.18.0",
    "@remix-run/dev": "1.11.1",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "npm-run-all": "4.1.5",
    "typescript": "4.8.4",
    "wrangler": "2.2.1"
  }
}
